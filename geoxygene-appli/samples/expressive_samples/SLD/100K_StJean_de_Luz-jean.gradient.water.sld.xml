<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<StyledLayerDescriptor xmlns:ogc="http://www.opengis.net/ogc">
	<Background>
		<URI>../images/canvas-bg.png</URI>
		<PaperHeightInCm>4.0</PaperHeightInCm>
		<PaperReferenceMapScale>100000.0</PaperReferenceMapScale>
		<Color>#FFFFFF</Color>
		<SimpleTexture XRepeat="false" YRepeat="false">
			<Displacement x="0.0" y="0.0"></Displacement>
			<ScaleFactor x="1.0" y="1.0"></ScaleFactor>
			<Rotation Angle="0.0"></Rotation>
			<URI>../images/canvas-bg.png</URI>
		</SimpleTexture>

	</Background>
	<NamedLayer>
		<Name>mer_OCS_100</Name>
		<UserStyle>
			<Name>Style créé pour le layer mer_OCS_100</Name>
			<IsDefault>false</IsDefault>
			<FeatureTypeStyle>
				<Rule>
					<LegendGraphic>
						<Graphic>
							<Mark>
								<WellKnownName>square</WellKnownName>
								<Fill>
									<CssParameter name="fill">#83f87c</CssParameter>
									<CssParameter name="fill-opacity">0.8</CssParameter>
								</Fill>
								<Stroke>
									<CssParameter name="stroke">#5bad56</CssParameter>
									<CssParameter name="stroke-opacity">0.8</CssParameter>
									<CssParameter name="stroke-width">1.0</CssParameter>
								</Stroke>
							</Mark>
							<opacity>1.0</opacity>
							<Size>6.0</Size>
							<Rotation>
								<Literal>0</Literal>
							</Rotation>
						</Graphic>
					</LegendGraphic>
					<PolygonSymbolizer uom="http://www.opengeospatial.org/se/units/metre">
						<Stroke>
							<CssParameter name="stroke">#5bad56</CssParameter>
							<CssParameter name="stroke-opacity">0.8</CssParameter>
							<CssParameter name="stroke-width">1.0</CssParameter>
						</Stroke>
						<GeometryPropertyName>geom</GeometryPropertyName>
						<Fill>
							<ExpressiveFill>
								<ExpressiveMethod>AnimatedSea</ExpressiveMethod>
								<ExpressiveParameter name="gradientTexture">
									<GradientTexture>
										<Resolution>72</Resolution>
										<MapScale>100000</MapScale>
										<MaxCoastlineLength>1000</MaxCoastlineLength>
										<BlurSize>10</BlurSize>
									</GradientTexture>
								</ExpressiveParameter>
							</ExpressiveFill>
							<CssParameter name="fill">#83f87c</CssParameter>
							<CssParameter name="fill-opacity">0.8</CssParameter>
						</Fill>
					</PolygonSymbolizer>
				</Rule>
			</FeatureTypeStyle>
		</UserStyle>
	</NamedLayer>
</StyledLayerDescriptor>
