<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<StyledLayerDescriptor xmlns:ogc="http://www.opengis.net/ogc">
	<Background>
		<url>./samples/expressive samples/images/canvas-bg.png</url>
		<PaperHeightInCm>4.0</PaperHeightInCm>
		<PaperReferenceMapScale>100000.0</PaperReferenceMapScale>
		<Color>#FFFFFF</Color>
	</Background>
	<NamedLayer>
		<Name>linear</Name>
		<UserStyle>
			<Name>Style créé pour le layer linear</Name>
			<IsDefault>false</IsDefault>
			<FeatureTypeStyle>
				<Rule>
					<LegendGraphic>
						<Graphic>
							<Mark>
								<WellKnownName>square</WellKnownName>
								<Fill>
									<CssParameter name="fill">#04ba36</CssParameter>
									<CssParameter name="fill-opacity">0.8</CssParameter>
								</Fill>
								<Stroke>
									<CssParameter name="stroke">#028225</CssParameter>
									<CssParameter name="stroke-opacity">0.8</CssParameter>
									<CssParameter name="stroke-width">1.0</CssParameter>
								</Stroke>
							</Mark>
							<opacity>1.0</opacity>
							<Size>6.0</Size>
							<Rotation>
								<Literal>0</Literal>
							</Rotation>
						</Graphic>
					</LegendGraphic>
					<LineSymbolizer uom="http://www.opengeospatial.org/se/units/metre">
						<Stroke>
							<CssParameter name="stroke">#000000</CssParameter>
							<CssParameter name="stroke-opacity">1.0</CssParameter>
							<CssParameter name="stroke-width">500.0</CssParameter>
							<CssParameter name="stroke-linejoin">round</CssParameter>
							<CssParameter name="stroke-linecap">round</CssParameter>
							<ExpressiveStroke>
								<ExpressiveMethod>dlyr</ExpressiveMethod>
								<ExpressiveParameter name="brushStartWidth">136</ExpressiveParameter>
                                <ExpressiveParameter name="brushEndWidth">135</ExpressiveParameter>
								<ExpressiveParameter name="transitionSize">200</ExpressiveParameter>
								<ExpressiveParameter name="paperReferenceMapScale">100000.0</ExpressiveParameter>
								<ExpressiveParameter name="paperRoughness">0.0</ExpressiveParameter>
								<ExpressiveParameter name="brushRoughness">5</ExpressiveParameter>
								<ExpressiveParameter name="strokePressure">4</ExpressiveParameter>
								<ExpressiveParameter name="strokeSoftness">0.4</ExpressiveParameter>
								<ExpressiveParameter name="PaperSizeInCm">4</ExpressiveParameter>
								<ExpressiveParameter name="strokeWidth">1.0</ExpressiveParameter>
								<ExpressiveParameter name="paperStrength">1.0</ExpressiveParameter>
								<ExpressiveParameter name="brushStrength">1.2</ExpressiveParameter>
								<ExpressiveParameter name="brushSmoothness">0.483</ExpressiveParameter>
								<ExpressiveParameter name="paperTexture">
									<SimpleTexture XRepeat="false" YRepeat="false">
										<Displacement x="0.0" y="0.0"></Displacement>
										<ScaleFactor x="1.0" y="1.0"></ScaleFactor>
										<Rotation Angle="0.0"></Rotation>
										<URI>../images/canvas-normalized.png</URI>
									</SimpleTexture>
								</ExpressiveParameter>
								<ExpressiveParameter name="brushTexture">
									<SimpleTexture XRepeat="false" YRepeat="false">
										<Displacement x="0.0" y="0.0"></Displacement>
										<ScaleFactor x="1.0" y="1.0"></ScaleFactor>
										<Rotation Angle="0.0"></Rotation>
										<URI>../images/pencil1-tiled-136-135.png</URI>
									</SimpleTexture>
								</ExpressiveParameter>
							</ExpressiveStroke>
						</Stroke>
						<GeometryPropertyName>geom</GeometryPropertyName>
						<PerpendicularOffset>0.0</PerpendicularOffset>
					</LineSymbolizer>
				</Rule>
			</FeatureTypeStyle>
		</UserStyle>
	</NamedLayer>
</StyledLayerDescriptor>
