<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	
	<!-- ========================================================================== -->
	<!-- 	WPS Service -->
	<!-- ========================================================================== -->
	<!-- CarteTopo WPS  -->
	<bean id="CreateFaces"   class="fr.ign.cogit.geoxygene.wps.contrib.cartetopo.CreateFaces" />
	<bean id="ImportAsEdges" class="fr.ign.cogit.geoxygene.wps.contrib.cartetopo.ImportAsEdgesProcess" />
	
	<!-- DataMatching WPS -->
	<bean id="networkDataMatchingProcess" class="fr.ign.cogit.geoxygene.wps.contrib.datamatching.NetworkDataMatchingProcess" />
	<bean id="appariementReseau1Process"  class="fr.ign.cogit.geoxygene.wps.contrib.datamatching.AppariementReseau1Process" />
	<bean id="distanceProcess"            class="fr.ign.cogit.geoxygene.wps.contrib.datamatching.DistanceProcess" />
	

	<!-- ========================================================================== -->
	<!-- 	The PPIO -->
	<!-- ========================================================================== -->
	<bean id="networkDataMatchingResultPPIO" 
			class="fr.ign.cogit.geoxygene.wps.contrib.datamatching.ppio.NetworkDataMatchingResultPPIO" />
			
	<bean id="paramDistanceNetworkDataMatchingPPIO"
	       class="fr.ign.cogit.geoxygene.wps.contrib.datamatching.ppio.ParamDistanceNetworkDataMatchingPPIO" />


	<!-- ========================================================================== -->
	<!-- 	Process Factory -->
	<!-- ========================================================================== -->
    <bean id="cogitProcessFactory" class="fr.ign.cogit.geoxygene.wps.SpringBeanProcessFactory">
    	<constructor-arg index="0" value="My Custom WPS Processes"/>
        <constructor-arg index="1" value="mywps"/>
        <constructor-arg index="2" value="fr.ign.cogit.geoxygene.wps.COGITProcess"/>
	</bean>


</beans>